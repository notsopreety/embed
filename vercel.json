{
  "version": 2,
  "builds": [
    {
      "src": "server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/embed/(.*)",
      "dest": "server.ts"
    },
    {
      "src": "/proxy(.*)",
      "dest": "server.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "server.ts"
    },
    {
      "src": "/(.*\\.(html|css|js|ico|png|jpg|svg))",
      "dest": "public/$1"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    }
  ]
}
